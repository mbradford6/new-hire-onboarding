---
component: navbar
---
	<div class="navbar sidebar-wrapper col-md-2" role="navigation">
		<div class="container-fluid">
			<div class="navbar-header">
				<button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
				<span class="sr-only">Toggle navigation</span>
				<span class="icon-bar"></span>
				<span class="icon-bar"></span>
				<span class="icon-bar"></span>
				</button>
			</div>
			<div class="navbar-collapse nav-stacked collapse">
				<ul class="nav">
					<h3>Getting Started</h3>
					{{#each pages}}
						{{#is data.section "gettingStarted"}}
							<li{{#if this.isCurrentPage}} class="active"{{/if}}>
								<a href="{{relative dest this.dest}}">{{ data.title }}</a>
							</li>
						{{/is}}
					{{/each}}
					<h3>Tutorials</h3>
					{{#each pages}}
						{{#is data.section "tutorials"}}
							<li{{#if this.isCurrentPage}} class="active"{{/if}}>
								<a href="{{relative dest this.dest}}">{{ data.title }}</a>
							</li>
						{{/is}}
					{{/each}}
					<h3>Going Further</h3>
					{{#each pages}}
						{{#is data.section "goingFurther"}}
							<li{{#if this.isCurrentPage}} class="active"{{/if}}>
								<a href="{{relative dest this.dest}}">{{ data.title }}</a>
							</li>
						{{/is}}
					{{/each}}
				</ul>
			</div>
		</div>
</div>